                                              IsEnabled="{Binding Source={StaticResource isEn}, Path=IsEnabled, Mode=OneWay}">
                                        <ComboBoxItem Content="Single (auto-completes)" ></ComboBoxItem>
                                        <ComboBoxItem Content="Multiple (no auto-completion)"></ComboBoxItem>
                                        <ComboBoxItem Content="Multiple: auto complete (code level)"></ComboBoxItem>
                                        <ComboBoxItem Content="Multiple: auto complete (include / exclude level)"></ComboBoxItem>
                                        <ComboBoxItem Content="Multiple: safety first"></ComboBoxItem>
                                    </ComboBox>
                                    <CheckBox x:Name="cbScreeningListIsGood" Grid.Column="0" Grid.Row="8" Margin="4,8,4,2" IsChecked="{Binding ScreeningListIsGood, Mode=OneWay}" Content="Screening list is Present" ToolTipService.ToolTip="True when there currently is a screening list with items left to screen" 
                                              HorizontalAlignment="Right" IsEnabled="False"
                                              ></CheckBox>
                                    <CheckBox x:Name="cbScreeningAutoExclude" Grid.Column="1" Grid.Row="8" Margin="4,8,4,2" IsChecked="{Binding ScreeningAutoExclude, Mode=TwoWay}" Content="Auto-exclude" ToolTipService.ToolTip="Check this box to automatically exclude citations as you go" ></CheckBox>
                                    <CheckBox x:Name="cbScreeningFullIndex" Grid.Column="1" Grid.Row="9" Margin="4,8,4,2" IsChecked="{Binding ScreeningIndexed, Converter={StaticResource ReverseBoolean}, Mode=TwoWay}" Content="Review needs indexing"
                                              IsEnabled="{Binding Source={StaticResource isEn}, Path=IsEnabled, Mode=OneWay}" ToolTipService.ToolTip="Tick this box if you want the review data to be re-indexed; e.g. you have uploaded new citations since creating your screening list"></CheckBox>
                                    <CheckBox x:Name="cbScreeningTrainingRunning" Grid.Column="1" Grid.Row="10" Margin="4,8,4,2" IsChecked="{Binding ScreeningModelRunning}" Content="List creation in progress" IsEnabled="False" ToolTipService.ToolTip="If checked, the machine learning model is running. Refresh to get current status."></CheckBox>
                                    <Button x:Name="cmdScreeningRefreshReviewInfo" Grid.Column="1" Grid.Row="11" Width="24" HorizontalAlignment="Left" Margin="4,4,0,4" Click="cmdScreeningRefreshReviewInfo_Click">
                                            <Image Width="16" Height="16" Source="Icons/view-refresh-7.png" ToolTipService.ToolTip="Refresh screening options status" />
                                    </Button>
                                    <StackPanel Grid.Column="0" Grid.Row="10" Orientation="Horizontal">
                                        <Button x:Name="cmdScreeningRunSimulation" Content="Run simulation" Margin="12,5,2,5" Width="90" Click="cmdScreeningRunSimulation_Click"></Button>
                                        <Button x:Name="cmdScreeningSimulationSave" Content="Download results" Margin="2,5,2,5" Width="100" Click="cmdScreeningSimulationSave_Click"></Button>
                                        <Button x:Name="cmdScreeningSimulationResults" Content="Save results" Margin="2,5,2,5" Visibility="Collapsed" Width="80" Click="cmdScreeningSimulationResults_Click"></Button>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal" Grid.Column="0" Grid.Row="11" HorizontalAlignment="Left">
                                        <Button x:Name="cmdScreeningSaveReviewOptions"   Margin="8,3,4,3" HorizontalAlignment="Stretch"
                                                Click="cmdScreeningSaveReviewOptions_Click" IsEnabled="{Binding UserCanEditScreeningSetting, Mode=OneWay}">
                                            <StackPanel Orientation="Horizontal">
                                                <Image Width="16" Height="16" Source="Icons/DocumentSave.png"/>
                                                <TextBlock Margin="8,0,2,0"> Save Options </TextBlock>
                                            </StackPanel>
                                        </Button>
                                        <Button x:Name="cmdScreeningCancelReviewOptions" Content="Cancel" Width="100" Margin="4,3,8,3" HorizontalAlignment="Right"
                                                ToolTipService.ToolTip="You can also click 'Refresh'"
                                                Click="cmdScreeningCancelReviewOptions_Click" IsEnabled="{Binding UserCanEditScreeningSetting, Mode=OneWay}"></Button>
                                    </StackPanel>
                                </Grid>

                                <telerikGridView:RadGridView x:Name="GridViewScreeningCodes" AutoGenerateColumns="False" ShowGroupPanel="False" Margin="6,0,0,0"
                                    HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Grid.Column="0" Grid.Row="3"
                                    RowIndicatorVisibility="Collapsed" ItemsSource="{Binding Source={StaticResource TrainingScreeningCriteriaData}, Path=Data}"
                                    SelectionMode="Single" IsEnabled="{Binding Source={StaticResource isEn}, Path=IsEnabled, Mode=OneWay}" >
                                    <telerikGridView:RadGridView.Columns>
                                        <telerikGridView:GridViewDataColumn Header="Code" DataMemberBinding="{Binding AttributeName}" Width="*" IsReadOnly="True"></telerikGridView:GridViewDataColumn>
                                        <telerikGridView:GridViewDataColumn Header="Include?" DataMemberBinding="{Binding Included}" Width="60" IsReadOnly="False"></telerikGridView:GridViewDataColumn>
                                        <telerikGridView:GridViewDataColumn IsFilterable="False" IsSortable="False" CellStyle="{StaticResource cellStyleDeleteTrainingScreeningCriteria}" Width="60" Header=""></telerikGridView:GridViewDataColumn>
                                    </telerikGridView:RadGridView.Columns>
                                </telerikGridView:RadGridView>

                                <telerikNavigation:RadToolBar Grid.Column="0" Grid.Row="2" HorizontalAlignment="Stretch" OverflowButtonVisibility="Collapsed"
				                        VerticalAlignment="Stretch" >
                                    <Button x:Name="cmdAddAttributeToScreeningCriteria" Click="cmdAddAttributeToScreeningCriteria_Click" IsEnabled="{Binding UserCanEditScreeningSetting, Mode=OneWay}" >
                                        <StackPanel Orientation="Horizontal">
                                            <Image Width="22" Height="22" Source="Icons/arrow-down.png"
						                    ToolTipService.ToolTip="Add to screening criteria" />
                                            <TextBlock Text="Add selected code" Margin="5,4,4,4"></TextBlock>
                                        </StackPanel>
                                    </Button>
                                    <CheckBox x:Name="cbScreeningAddAttributesAutomatically" Margin="5,5,5,5" IsChecked="True" 
                                              Content="Add codes automatically" IsEnabled="{Binding UserCanEditScreeningSetting, Mode=OneWay}" ></CheckBox>
                                </telerikNavigation:RadToolBar>

                                <telerikNavigation:RadToolBar Grid.Column="1" Grid.Row="0" HorizontalAlignment="Stretch" OverflowButtonVisibility="Collapsed"
				                        VerticalAlignment="Stretch" >
                                    <TextBlock Text="Screening progress"></TextBlock>
                                </telerikNavigation:RadToolBar>

                                <telerik:RadCartesianChart x:Name="chart" Grid.Column="1" Grid.Row="1" Margin="15,15,15,15">
                                    <telerik:RadCartesianChart.Behaviors>
                                        <telerik:ChartTrackBallBehavior ShowIntersectionPoints="True"
                                             ShowTrackInfo="False"/>
                                    </telerik:RadCartesianChart.Behaviors>
                                    <telerik:RadCartesianChart.HorizontalAxis>
                                        <telerik:LinearAxis Title="Number of items screened" 
                                    Minimum="0" 
                                    LastLabelVisibility="Hidden"/>
                                    </telerik:RadCartesianChart.HorizontalAxis>
                                    <telerik:RadCartesianChart.VerticalAxis>
                                        <telerik:LinearAxis Title="Number of relevant items found"/>
                                    </telerik:RadCartesianChart.VerticalAxis>
                                    <telerik:RadCartesianChart.Series>
                                        <telerik:ScatterLineSeries XValueBinding="TotalN"
                                           YValueBinding="TotalIncludes" 
                                           ItemsSource="{Binding Source={StaticResource TrainingListData}, Path=Data}">
                                            <telerik:ScatterLineSeries.TrackBallTemplate>
                                                <DataTemplate>
                                                    <StackPanel>
                                                        <Rectangle Width="5"
                                                            Height="5"
                                                            Fill="Red"
                                                            RenderTransformOrigin="0.5,0.5">
                                                            <Rectangle.RenderTransform>
                                                                <RotateTransform Angle="45"/>
                                                            </Rectangle.RenderTransform>
                                                        </Rectangle>
                                                        <Border Background="Bisque">
                                                            <TextBlock Text="{Binding DisplayContent}"/>
                                                        </Border>
                                                    </StackPanel>
                                                </DataTemplate>
                                            </telerik:ScatterLineSeries.TrackBallTemplate>
                                        </telerik:ScatterLineSeries>
                                    </telerik:RadCartesianChart.Series>
                                </telerik:RadCartesianChart>

                                <telerikNavigation:RadToolBar Grid.Column="1" Grid.Row="2" HorizontalAlignment="Stretch" OverflowButtonVisibility="Collapsed"
				                        VerticalAlignment="Stretch" >
                                    <TextBlock Text="Screening history"></TextBlock>
                                </telerikNavigation:RadToolBar>

                                <telerikGridView:RadGridView x:Name="GridViewTrainingRecord" AutoGenerateColumns="False" ShowGroupPanel="False"
                                    HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Grid.Column="1" Grid.Row="3" Margin="15,5,10,5"
                                    RowIndicatorVisibility="Collapsed" ItemsSource="{Binding Source={StaticResource TrainingListData}, Path=Data}"
                                    SelectionMode="Single">

                                    <telerikGridView:RadGridView.Resources>
                                        <Style TargetType="telerik:GridViewRow">
                                            <Setter Property="ToolTipService.ToolTip" Value="{Binding ContactName, StringFormat='Training triggered by: \{0\}'}" />
                                        </Style>
                                    </telerikGridView:RadGridView.Resources>

                                    <telerikGridView:RadGridView.Columns>
                                        <telerikGridView:GridViewDataColumn Header="No" IsFilterable="False" DataMemberBinding="{Binding Iteration}" Width="45" IsReadOnly="True"></telerikGridView:GridViewDataColumn>
                                        <telerikGridView:GridViewDataColumn Header="Started" IsFilterable="False" DataMemberBinding="{Binding StartTime}" Width="*" IsReadOnly="True"></telerikGridView:GridViewDataColumn>
                                        <telerikGridView:GridViewDataColumn Header="Reviewer" IsVisible="False" IsFilterable="False" DataMemberBinding="{Binding ContactName}" Width="*" IsReadOnly="True"></telerikGridView:GridViewDataColumn>
                                        <telerikGridView:GridViewDataColumn Header="TP" IsFilterable="False" DataMemberBinding="{Binding TP}" 
                                                                            Width="40" IsReadOnly="True">
                                            <telerik:GridViewDataColumn.HeaderCellStyle>
                                                <Style TargetType="telerik:GridViewHeaderCell">
                                                    <Setter Property="ToolTipService.ToolTip" Value="True Positives">
                                                    </Setter>
                                                </Style>
                                            </telerik:GridViewDataColumn.HeaderCellStyle>
                                        </telerikGridView:GridViewDataColumn>
                                        <telerikGridView:GridViewDataColumn Header="TN" IsFilterable="False" DataMemberBinding="{Binding TN}" 
                                                                            Width="40" IsReadOnly="True">
                                            <telerik:GridViewDataColumn.HeaderCellStyle>
                                                <Style TargetType="telerik:GridViewHeaderCell">
                                                    <Setter Property="ToolTipService.ToolTip" Value="True Negatives">
                                                    </Setter>
                                                </Style>
                                            </telerik:GridViewDataColumn.HeaderCellStyle>
                                        </telerikGridView:GridViewDataColumn>
                                    </telerikGridView:RadGridView.Columns>
                                </telerikGridView:RadGridView>

                                <sdk:GridSplitter Grid.Row="1" Grid.Column="1" 
                                       Width="8" HorizontalAlignment="Left" VerticalAlignment="Stretch" />

                                <sdk:GridSplitter Grid.Row="3" Grid.Column="1"
                                       Width="8" HorizontalAlignment="Left" VerticalAlignment="Stretch" />

                            </Grid>
                        </radDock:RadPane>

                    </radDock:RadPaneGroup>
                        
                </radDock:RadSplitContainer>
