<UserControl
    xmlns:telerikInput="clr-namespace:Telerik.Windows.Controls;assembly=Telerik.Windows.Controls.Input" 
    x:Class="EppiReviewer4.homeDocuments"
    xmlns:telerikGridView="clr-namespace:Telerik.Windows.Controls;assembly=Telerik.Windows.Controls.GridView"
    xmlns:grid="clr-namespace:Telerik.Windows.Controls.GridView;assembly=Telerik.Windows.Controls.GridView"
    xmlns:telerikNavigation="clr-namespace:Telerik.Windows.Controls;assembly=Telerik.Windows.Controls.Navigation" 
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:helper="clr-namespace:EppiReviewer4.Helpers"
    xmlns:sys="clr-namespace:System;assembly=mscorlib"
    xmlns:local="clr-namespace:EppiReviewer4;assembly=EppiReviewer4"
    xmlns:radDock="clr-namespace:Telerik.Windows.Controls;assembly=Telerik.Windows.Controls.Docking"
    xmlns:csla="clr-namespace:Csla.Silverlight;assembly=Csla"
    xmlns:cslaXAML="clr-namespace:Csla.Xaml;assembly=Csla.Xaml"
    xmlns:core="clr-namespace:Telerik.Windows.Controls;assembly=Telerik.Windows.Controls"
    xmlns:telerikData="clr-namespace:Telerik.Windows.Data;assembly=Telerik.Windows.Data"
    xmlns:my="clr-namespace:MindFusion.Diagramming.Silverlight;assembly=MindFusion.Diagramming.Silverlight"
    xmlns:dragDrop="clr-namespace:Telerik.Windows.Controls.DragDrop;assembly=Telerik.Windows.Controls"
    xmlns:dpager="clr-namespace:Telerik.Windows.Controls;assembly=Telerik.Windows.Controls.Data"
    xmlns:telerik="http://schemas.telerik.com/2008/xaml/presentation"
    xmlns:sdk="http://schemas.microsoft.com/winfx/2006/xaml/presentation/sdk"
    xmlns:chart="clr-namespace:Telerik.Windows.Controls;assembly=Telerik.Windows.Controls.Chart"
    xmlns:chartView="clr-namespace:Telerik.Windows.Controls.ChartView;assembly=Telerik.Windows.Controls.Chart"
    xmlns:circulargraph ="clr-namespace:CircularRelationshipGraph;assembly=CircularRelationshipGraph"
    >
    <UserControl.Resources> 
        <helper:SourceButtonConverter x:Key="helper"/>
        <helper:SourceNameConverter x:Key="SourceNameConverter"/>
        <helper:SourceButtonTipConverter x:Key="SourceTipHelper"/>
        <helper:SourceNameStyleConverter x:Key="SourceNameStyleConverter"/>
        <helper:ReverseBooleanConverter x:Key="ReverseBoolean" />
        <helper:EditTrueFalseVisibilityConverter x:Key="EditTrueFalseVisibility" />
        <helper:ReverseEditTrueFalseVisibilityConverter x:Key="ReverseEditTrueFalseVisibility" />
        <!--<Button x:Name="isEn" IsEnabled="{Binding HasWriteRights, Mode=OneWay}" ></Button>-->
        
        <!--helper:EnableButtonConverter x:Key="enableHelper"/-->
        <core:HierarchicalDataTemplate x:Key="AttributeTemplate" ItemsSource = "{Binding Attributes}">
            <StackPanel Orientation="Horizontal">
                <TextBlock Text="{Binding AttributeName}" />
            </StackPanel>
        </core:HierarchicalDataTemplate>

        <core:HierarchicalDataTemplate x:Key="CodeSet" ItemTemplate="{StaticResource AttributeTemplate}" ItemsSource = "{Binding Attributes}">
            <StackPanel Orientation="Horizontal">
                <TextBlock Text="{Binding SetName}"  />
            </StackPanel>
        </core:HierarchicalDataTemplate>
        
        <!--csla:CslaDataProvider x:Key="WriteRights"
                           ManageObjectLifetime="True"
                           IsInitialLoadEnabled="True"
                           ObjectType="BusinessLibrary.Security.ReviewerIdentity, BusinessLibrary"
                           FactoryMethod="newEnabler"
                           DataChanged="CslaDataProvider_EnablerDataChanged"
                           /-->
        <cslaXAML:CslaDataProvider x:Key="ItemListData"
                           ManageObjectLifetime="True"
                           IsInitialLoadEnabled="False"
                           ObjectType="BusinessLibrary.BusinessClasses.ItemList, BusinessLibrary, Version=..., Culture=neutral, PublicKeyToken=null"
                           FactoryMethod="GetItemList"
                                    
                           DataChanged="CslaDataProviderItemList_DataChanged"
                           PropertyChanged="CslaDataProvider_PropertyChanged"
                           />
        <cslaXAML:CslaDataProvider x:Key="SourcesData"
                           ManageObjectLifetime="True"
                           IsInitialLoadEnabled="False"
                           ObjectType="BusinessLibrary.BusinessClasses.ReadOnlySourceList, BusinessLibrary, Version=..., Culture=neutral, PublicKeyToken=null"
                           FactoryMethod="GetSources" 
                           DataChanged="CslaDataProvider_DataChanged"
                           />
        
        
        <cslaXAML:CslaDataProvider x:Key="MetaAnalysisListData"
                           ManageObjectLifetime="True"
                           IsInitialLoadEnabled="False"
                           ObjectType="BusinessLibrary.BusinessClasses.MetaAnalysisList, BusinessLibrary, Version=..., Culture=neutral, PublicKeyToken=null"
                           FactoryMethod="GetMetaAnalysisList"
                           DataChanged="CslaDataProviderMetaAnalysisListData_DataChanged"
                           />
        <!--<cslaXAML:CslaDataProvider x:Key="ReviewContactNVLData"
                           ManageObjectLifetime="True"
                           IsInitialLoadEnabled="True"
                           ObjectType="BusinessLibrary.BusinessClasses.ReviewContactNVL, BusinessLibrary, Version=..., Culture=neutral, PublicKeyToken=null"
                           FactoryMethod="GetReviewContactNVL"
                           DataChanged="CslaDataProvider_ReviewContactNVLDataDataChanged"
                           />-->
        <cslaXAML:CslaDataProvider x:Key="ReviewContactListData"
                           ManageObjectLifetime="True"
                           IsInitialLoadEnabled="False"
                           ObjectType="BusinessLibrary.BusinessClasses.ReviewContactList, BusinessLibrary, Version=..., Culture=neutral, PublicKeyToken=null"
                           FactoryMethod="GetReviewContactList"
                           DataChanged="ReviewContactListData_DataChanged"
                           />
        <cslaXAML:CslaDataProvider x:Key="WorkAllocationListData"
                           ManageObjectLifetime="True"
                           IsInitialLoadEnabled="False"
                           ObjectType="BusinessLibrary.BusinessClasses.WorkAllocationList, BusinessLibrary, Version=..., Culture=neutral, PublicKeyToken=null"
                           FactoryMethod="GetWorkAllocationList"
                           DataChanged="CslaDataProvider_WorkAllocationListDataDataChanged"
                           />
        <!--<cslaXAML:CslaDataProvider x:Key="WorkAllocationContactListData"
                           ManageObjectLifetime="True"
                           IsInitialLoadEnabled="False"
                           ObjectType="BusinessLibrary.BusinessClasses.WorkAllocationContactList, BusinessLibrary, Version=..., Culture=neutral, PublicKeyToken=null"
                           FactoryMethod="GetWorkAllocationContactList"
                           DataChanged="CslaDataProvider_WorkAllocationContactListDataDataChanged"
                           />-->
        <!--<cslaXAML:CslaDataProvider x:Key="ReviewsData"
                           ManageObjectLifetime="True"
                           IsInitialLoadEnabled="False"
                           ObjectType="BusinessLibrary.BusinessClasses.ReadOnlyReviewList, BusinessLibrary, Version=..., Culture=neutral, PublicKeyToken=null"
                           DataChanged="CslaDataProvider_DataChanged"
                           FactoryMethod="GetReviewList"/>-->
        <cslaXAML:CslaDataProvider x:Key="ComparisonListData"
                           ManageObjectLifetime="True"
                           IsInitialLoadEnabled="False"
                           ObjectType="BusinessLibrary.BusinessClasses.ComparisonList, BusinessLibrary, Version=..., Culture=neutral, PublicKeyToken=null"
                           DataChanged="ComparisonListData_DataChanged"
                           FactoryMethod="GetComparisonList"/>
        <cslaXAML:CslaDataProvider x:Key="ItemAttributeCrosstabListData"
                           ManageObjectLifetime="True"
                           IsInitialLoadEnabled="False"
                           ObjectType="BusinessLibrary.BusinessClasses.ReadOnlyItemAttributeCrosstabList, BusinessLibrary, Version=..., Culture=neutral, PublicKeyToken=null"
                           DataChanged="ItemAttributeCrosstabListData_DataChanged"
                           FactoryMethod="GetItemAttributeChildCrosstabList"/>
        <cslaXAML:CslaDataProvider x:Key="TrainingScreeningCriteriaData"
                           ManageObjectLifetime="True"
                           IsInitialLoadEnabled="False"
                           ObjectType="BusinessLibrary.BusinessClasses.TrainingScreeningCriteriaList, BusinessLibrary, Version=..., Culture=neutral, PublicKeyToken=null"
                           DataChanged="TrainingScreeningCriteriaData_DataChanged"
                           FactoryMethod="GetTrainingScreeningCriteriaList"/>
        
        <Style TargetType="grid:GridViewCell" x:Name="cellStyle">
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate>
                        <Button x:Name="cmdEditCoding" Content="Go" Click="cmdEditCoding_Click" ></Button>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
            <!-- Remove this style if the default select cell will do
        <Style TargetType="grid:GridViewCell" x:Name="cellStyleSelected">
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate>
                        <CheckBox x:Name="CheckBoxSelectedItem" HorizontalAlignment="Center" VerticalAlignment="Center" IsChecked="{Binding Path=IsSelected, Mode=TwoWay}" Margin="2,0,0,0"></CheckBox>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
        -->
        <Style TargetType="grid:GridViewCell" x:Name="cellSearchStyle">
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate>
                        <StackPanel HorizontalAlignment="Left" Orientation="Horizontal">
                            <Button x:Name="cmdListSearch" Content="Select" Click="cmdListSearch_Click"></Button>
                            <Button x:Name="cmdVisualiseSearch" Content="Visualise" Click="cmdVisualiseSearch_Click" 
                                    IsEnabled="{Binding IsClassifierResult}"
                                    Visibility="{Binding IsClassifierResult, Converter={StaticResource EditTrueFalseVisibility}, Mode=OneWay}"></Button>
                        </StackPanel>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
        <Style TargetType="grid:GridViewCell" x:Name="cellS9_del_S">
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate>
                        <Button x:Name="cmdDelSource" Content="{Binding Items, Converter={StaticResource helper}}" Click="cmdDelSource_Click" ></Button>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
        <Style TargetType="grid:GridViewCell" x:Name="cellS_Undel_S">
            <Setter Property="Template" >
                <Setter.Value>
                    <ControlTemplate x:Name="cellS_Undel_T">
                        <Button x:Name="cmdDelSource" Content="Undelete" Click="cmdDelSource_Click" ></Button>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
       <Style TargetType="grid:GridViewCell" x:Name="cellStyleListSourceItems">
            <Setter Property="Template" >
                <Setter.Value>
                    <ControlTemplate>
                        <Button x:Name="cmdListSourceItems" Content="List" Click="cmdListSourceItems_Click" ></Button>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style> 
        <Style TargetType="grid:GridViewCell" x:Name="cellStyleFrequencies">
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate>
                        <Button x:Name="cmdReportFrequencies" Content="Go" Click="cmdReportFrequencies_Click"></Button>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
        <Style TargetType="grid:GridViewCell" x:Name="cellStyleEditMetaAnalysis">
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate>
                        <Button x:Name="cmdEditMetaAnalysis" Content="Edit/Run" Click="cmdEditMetaAnalysis_Click" ></Button>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
        <Style TargetType="grid:GridViewCell" x:Name="cellStyleDeleteMetaAnalysis">
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate>
                        <Button x:Name="cmdDeleteMetaAnalysis" Content="Delete" Click="cmdDeleteMetaAnalysis_Click"
                                 IsEnabled="{Binding Source={StaticResource isEn}, Path=IsEnabled, Mode=OneWay}"></Button>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
        <Style TargetType="grid:GridViewCell" x:Name="cellStyleRunMetaAnalysis">
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate>
                        <Button x:Name="cmdRunMetaAnalysis" Content="Run (old method)" Click="cmdRunMetaAnalysis_Click"></Button>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
        <Style TargetType="grid:GridViewCell" x:Name="cellStyleDeleteWorkAllocation">
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate>
                        <Button x:Name="cmdDeleteWorkAllocation" Content="Delete" Click="cmdDeleteWorkAllocation_Click"
                                IsEnabled="{Binding Source={StaticResource isEn}, Path=IsEnabled, Mode=OneWay}"></Button>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
        
        <Style TargetType="grid:GridViewCell" x:Name="deleteComparisonCellStyle">
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate>
                        <Button x:Name="cmdDeleteComparison" Content="Delete" Click="cmdDeleteComparison_Click" 
                                IsEnabled="{Binding Source={StaticResource isEn}, Path=IsEnabled, Mode=OneWay}"></Button>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
        <Style TargetType="grid:GridViewCell" x:Name="quickReportComparisonCellStyle">
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate>
                        <Button x:Name="cmdQuickReportComparison" Content="Run" Click="cmdQuickReportComparison_Click" ></Button>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
        <Style TargetType="grid:GridViewCell" x:Name="statsComparisonCellStyle">
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate>
                        <Button x:Name="cmdStatsComparison" Content="View" Click="cmdStatsComparison_Click"></Button>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
        <Style TargetType="grid:GridViewCell" x:Name="cellStyleDeleteTrainingScreeningCriteria">
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate>
                        <Button x:Name="cmdDeleteTrainingScreeningCriteria" IsEnabled="{Binding Source={StaticResource isEn}, Path=IsEnabled, Mode=OneWay}"
                                Content="Delete" Click="cmdDeleteTrainingScreeningCriteria_Click" ></Button>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
        

    </UserControl.Resources>

    <Grid>
        <Grid.Resources>
            <Style x:Key="boolColumnStyle" TargetType="grid:GridViewCell">
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="grid:GridViewCell">
                            <Border HorizontalAlignment="Stretch">
                                <CheckBox HorizontalAlignment="Center" VerticalAlignment="Center" IsChecked="{Binding Path=IsSelected, Mode=TwoWay}" />
                            </Border>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>
        </Grid.Resources>
        
        <radDock:RadDocking>
            <radDock:RadDocking.DocumentHost>
                <radDock:RadSplitContainer >

                    <radDock:RadPaneGroup x:Name="DocumentListPane" SelectionChanged="DocumentListPane_SelectionChanged">
                        <radDock:RadPane Header="Documents" CanUserClose="False" CanFloat="False">
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="20"></RowDefinition>
                                    <RowDefinition MinHeight="20" MaxHeight="40"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="*"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                </Grid.RowDefinitions>

                                <TextBlock x:Name="TextBlockDocCount" Grid.Row="0" ></TextBlock>
                                <cslaXAML:BusyAnimation x:Name="loadingDataAnimation"  Grid.Row="0" IsRunning="{Binding Source={StaticResource ItemListData}, Path=IsBusy}" HorizontalAlignment="Right" />

                                <TextBox x:Name="TextBlockShowing" IsReadOnly="True" VerticalAlignment="Stretch" Grid.Row="1" TextWrapping="Wrap" VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Hidden"></TextBox>
                                <telerikGridView:RadGridView x:Name="ItemsGrid" Grid.Row="3" AutoGenerateColumns="False" ShowGroupPanel="False"
                                    IsReadOnly="True" MinHeight="50" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" IsEnabled="{Binding Source={StaticResource ItemListData}, Path=IsBusy, Converter={StaticResource ReverseBoolean}}"
                                    ItemsSource="{Binding PagedSource, ElementName=ItemsGridDataPager}" RowIndicatorVisibility="Collapsed"
                                    Filtered="ItemsGrid_Filtered" SelectionMode="Extended" FrozenColumnCount="1" ColumnWidth="*"
                                    IsSynchronizedWithCurrentItem="False">
                                    <telerikGridView:RadGridView.Columns>
                                        <telerikGridView:GridViewDataColumn IsFilterable="False" IsSortable="False" CellStyle="{StaticResource cellStyle}" Width="45" Header="" DataMemberBinding="{Binding Title}" ></telerikGridView:GridViewDataColumn>
                                        <telerikGridView:GridViewSelectColumn UniqueName="columnSelectItem" Width="Auto">
                                            <telerikGridView:GridViewSelectColumn.Header>
                                                <CheckBox Click="CheckBox_Click" Tag="ItemsGrid" />
                                            </telerikGridView:GridViewSelectColumn.Header>
                                        </telerikGridView:GridViewSelectColumn>
                                        <telerikGridView:GridViewDataColumn Header="" DataMemberBinding="{Binding ItemStatus}" IsFilterable="False" Width="14">
                                            <telerikGridView:GridViewDataColumn.ToolTipTemplate>
                                                <DataTemplate>
                                                    <TextBlock Text="{Binding ItemStatusTooltip}"/>
                                                </DataTemplate>
                                            </telerikGridView:GridViewDataColumn.ToolTipTemplate>
                                        </telerikGridView:GridViewDataColumn>
                                        <telerikGridView:GridViewDataColumn x:Name="DataColumnId" Header="ID" DataMemberBinding="{Binding ItemId}" ShowDistinctFilters="False" Width="60" IsVisible="False"></telerikGridView:GridViewDataColumn>
                                        <telerikGridView:GridViewDataColumn x:Name="DataColumnOldId" Header="YourID" DataMemberBinding="{Binding OldItemId}" ShowDistinctFilters="False" Width="71" IsVisible="False"></telerikGridView:GridViewDataColumn>
                                        <telerikGridView:GridViewDataColumn x:Name="DataColumnAuthors" Header="Authors" DataMemberBinding="{Binding Authors}" ShowDistinctFilters="False" Width="100" ></telerikGridView:GridViewDataColumn>
                                        <telerikGridView:GridViewDataColumn x:Name="DataColumnTitle" Header="Title" DataMemberBinding="{Binding Title}" ShowDistinctFilters="False" Width="130*"></telerikGridView:GridViewDataColumn>
                                        <telerikGridView:GridViewDataColumn x:Name="DataColumnJournal" Header="Journal" DataMemberBinding="{Binding ParentTitle}" ShowDistinctFilters="True" Width="70" IsVisible="False"></telerikGridView:GridViewDataColumn>
                                        <telerikGridView:GridViewDataColumn x:Name="DataColumnShortTitle" Header="Short title" DataMemberBinding="{Binding ShortTitle}" ShowDistinctFilters="False" Width="70" IsVisible="False"></telerikGridView:GridViewDataColumn>
                                        <telerikGridView:GridViewDataColumn x:Name="DataColumnItemType" Header="Type" DataMemberBinding="{Binding TypeName}" ShowDistinctFilters="True" Width="90" IsVisible="False"></telerikGridView:GridViewDataColumn>
                                        <telerikGridView:GridViewDataColumn x:Name="DataColumnYear" Header="Year" DataMemberBinding="{Binding Year}" ShowDistinctFilters="True" Width="20*" MaxWidth="70" IsVisible="True"></telerikGridView:GridViewDataColumn>
                                        <telerikGridView:GridViewDataColumn x:Name="DataColumnScore" Header="Info" DataMemberBinding="{Binding AttributeAdditionalText}" ShowDistinctFilters="True" Width="70*" IsVisible="False"></telerikGridView:GridViewDataColumn>
                                        <telerikGridView:GridViewDataColumn x:Name="DataColumnRank" Header="Score" DataMemberBinding="{Binding Rank}" ShowDistinctFilters="True" Width="60" IsVisible="False"></telerikGridView:GridViewDataColumn>
                                    </telerikGridView:RadGridView.Columns>
                                </telerikGridView:RadGridView>

                                <dpager:RadDataPager Grid.Row="4" x:Name="ItemsGridDataPager" IsTotalItemCountFixed="True"
                                    DisplayMode="All" NumericButtonCount="10" Source="{Binding Source={StaticResource ItemListData}, Path=Data}"
                                    PageIndexChanging="ItemsGridDataPager_PageIndexChanging"></dpager:RadDataPager>

                                <telerikNavigation:RadToolBar Grid.Row="2" HorizontalAlignment="Stretch"
				                        VerticalAlignment="Top" x:Name="mainToolBar">

                                    <Button x:Name="cmdShowIncluded" Click="cmdShowIncluded_Click">
                                        <Image Width="22" Height="22" Source="Icons/ListIncluded.png"
						                    ToolTipService.ToolTip="List included documents" />
                                    </Button>
                                    
                                    <Button x:Name="cmdShowExcluded" Click="cmdShowExcluded_Click">
                                        <Image Width="22" Height="22" Source="Icons/ListExcluded.png"
						                    ToolTipService.ToolTip="List excluded documents" />
                                    </Button>
                                    
                                    <Button x:Name="cmdShowDeleted" Click="cmdShowDeleted_Click">
                                        <Image Width="22" Height="22" Source="Icons/ListDeleted.png"
						                    ToolTipService.ToolTip="List deleted documents" />
                                    </Button>

                                    <telerikNavigation:RadToolBarSeparator Margin="7,2,7,2" />

                                    <Button x:Name="cmdNewItem" Click="cmdNewItem_Click" IsEnabled="{Binding Source={StaticResource isEn}, Path=IsEnabled, Mode=OneWay}">
                                        <Image Width="22" Height="22" Source="Icons/NewItem.png"
						                    ToolTipService.ToolTip="Create new document record" />
                                    </Button>
                                    
                                    <Button IsEnabled="{Binding Source={StaticResource isEn}, Path=IsEnabled, Mode=OneWay}"  Click="btAddS_Click" Tag="true">
                                        <Image Width="22" Height="22" Source="Icons/Import.png"
						                    ToolTipService.ToolTip="Import references" />
                                    </Button>
                                    
                                    <Button x:Name="cmdExportToRIS" IsEnabled="False" Click="cmdExportToRIS_Click">
                                        <Grid>
                                            <Image Width="22" Height="22" Source="Icons/Export.png"
						                    ToolTipService.ToolTip="Export Selected References" />
                                            <cslaXAML:BusyAnimation x:Name="BusyExporting" Grid.Row="0" Grid.Column="1" ToolTipService.ToolTip="Export references"/>
                                        </Grid>
                                    </Button>

                                    <!--<Button x:Name="cmdExportItemsGrid" IsEnabled="True" Click="cmdExportItemsGrid_Click" Visibility="Collapsed">
                                        <Grid>
                                            <Image Width="22" Height="22" Source="Icons/Save.png"
						                    ToolTipService.ToolTip="Export current grid to html (for use in excel, word or web browser)" />
                                        </Grid>
                                    </Button>-->

                                    <telerikNavigation:RadToolBarSeparator Margin="7,2,7,2" />

                                    <Button x:Name="cmdAssignIncluded"  IsEnabled="{Binding Source={StaticResource isEn}, Path=IsEnabled, Mode=OneWay}"  Click="cmdAssignIncluded_Click">
                                        <Image Width="22" Height="22" Source="Icons/assign.png"
						                    ToolTipService.ToolTip="Assign documents to be included or excluded" />
                                    </Button>
                                    
                                    <Button x:Name="cmdDeleteSelectedItems" 
                                              IsEnabled="{Binding Source={StaticResource isEn}, Path=IsEnabled, Mode=OneWay}" Click="cmdDeleteSelectedItems_Click"  Tag="true">
                                        <Image Width="22" Height="22" Source="Icons/trash_full.png"
						                    ToolTipService.ToolTip="Delete selected documents" />
                                    </Button>
                                    
                                    <telerikNavigation:RadToolBarSeparator Margin="7,2,7,2" />

                                    <Button x:Name="cmdFindDuplicateItems" Click="cmdFindDuplicateItems_Click">
                                        <Image Width="22" Height="22" Source="Icons/ManageDuplicates.png"
						                    ToolTipService.ToolTip="Manage duplicates" />
                                    </Button>

                                    <Button x:Name="cmdOpenClusterDocumentWindow" Click="cmdOpenClusterDocumentWindow_Click" IsEnabled="{Binding Source={StaticResource isEn}, Path=IsEnabled, Mode=OneWay}">
                                        <Image Width="22" Height="22" Source="Icons/AutoCode.png"
						                    ToolTipService.ToolTip="Automatically generate codes" />
                                    </Button>
                                    <Button x:Name="cmdRandomAllocate" Click="cmdRandomAllocate_Click" IsEnabled="{Binding Source={StaticResource isEn}, Path=IsEnabled, Mode=OneWay}">
                                        <Image Width="22" Height="22" Source="Icons/RandomAssign.png"
						                    ToolTipService.ToolTip="Assign documents to specified codes randomly" />
                                    </Button>
                                    <telerikNavigation:RadToolBarSeparator Margin="7,2,7,2" />

                                    <Button x:Name="cmdBibliography" ToolTipService.ToolTip="List formatted bibliography / Export table">
                                        <telerik:RadContextMenu.ContextMenu>
                                            <telerik:RadContextMenu x:Name="ctxMenuTranslate" EventName="Click" ItemClick="cmdBibliography_Click" ScrollViewer.VerticalScrollBarVisibility="Visible"
                                                                     FontSize="11" MaxHeight="350"
                                                                    IconColumnWidth="0" Opening="ctxMenuTranslate_Opening">
                                                <telerik:RadMenuItem Tag="Harvard" Header="Harvard" />
                                                <telerik:RadMenuItem Tag="Chicago" Header="Chicago" />
                                                <telerik:RadMenuItem Tag="NICE" Header="NICE format" />
                                                <telerik:RadMenuItem x:Name="BLdoc" Tag="BL" Header="BL document supply requests" />
                                                <telerik:RadMenuItem x:Name="BLdocCC" Tag="BLCopyrightCleared" Header="BL document supply requests – copyright cleared" />
                                                <telerik:RadMenuItem x:Name="JustTheGrid" Tag="JustTheGrid" Header="Export Current page/selection" ToolTipService.ToolTip="Exports current table/selection to html (for use in excel, word or web browser)" />
                                            </telerik:RadContextMenu>
                                        </telerik:RadContextMenu.ContextMenu>
                                        <StackPanel Orientation="Horizontal">
                                            <Image Width="22" Height="22" Source="Icons/PrintCodeSet.png"/>
                                        </StackPanel>
                                    </Button>
                                    
                                    <Button x:Name="cmdOpenReportsWindow" Click="cmdOpenReportsWindow_Click" >
                                        <Image Width="22" Height="22" Source="Icons/RunReports.png"
						                    ToolTipService.ToolTip="Run reports" />
                                    </Button>

                                    <Button x:Name="cmdOpenWindowItemReportWriter" Click="cmdOpenWindowItemReportWriter_Click">
                                        <Image Width="22" Height="22" Source="Icons/ItemCodingReport.png"
						                    ToolTipService.ToolTip="Get coding report(s) for selected item(s) that have been 'Completed'" />
                                    </Button>

                                    <Button x:Name="cmdColumnSelect" Click="cmdColumnSelect_Click" >
                                        <Image Width="22" Height="22" Source="Icons/ColumnSelect.png"
						                    ToolTipService.ToolTip="Select the fields you want to display below" />
                                    </Button>
                                    <telerikNavigation:RadToolBarSeparator Margin="7,2,7,2" />

                                    <Button x:Name="cmdSearch" Click="cmdSearch_Click" Tag="DocumentsTab" IsEnabled="{Binding Source={StaticResource isEn}, Path=IsEnabled, Mode=OneWay}">
                                        <Image Width="22" Height="22" Source="Icons/Find.png"
						                    ToolTipService.ToolTip="Search for documents" />
                                    </Button>

                                    <Button x:Name="cmdSearchLikeThese" Click="cmdSearchLikeThese_Click" IsEnabled="{Binding Source={StaticResource isEn}, Path=IsEnabled, Mode=OneWay}" >
                                        <Image Width="22" Height="22" Source="Icons/FindLikeThese.png"
						                    ToolTipService.ToolTip="Find more documents like the ones listed" />
                                    </Button>

                                    <TextBlock Text="Filter:" ToolTipService.ToolTip="Filter currently loaded list"></TextBlock>
                                    <TextBox x:Name="TextBoxItemsGridFind" Width="60" TextChanged="TextBoxItemsGridFind_TextChanged" GotFocus="TextBoxItemsGridFind_GotFocus"  ToolTipService.ToolTip="Filter currently loaded list"></TextBox>

                                </telerikNavigation:RadToolBar>
                                
                            </Grid>
                        </radDock:RadPane>

