                                    <telerikGridView:RadGridView.Columns>
                                    <telerikGridView:GridViewDataColumn IsFilterable="False" Header="" IsSortable="False" CellStyle="{StaticResource cellStyleListSourceItems}" 
                                                                        IsReorderable="False" IsResizable="False" Width="61"  DataMemberBinding="{Binding Source_Name}">
                                        <telerikGridView:GridViewColumn.Footer >
                                            <TextBlock FontWeight="Bold" Text="TOTALS:"/>
                                        </telerikGridView:GridViewColumn.Footer>
                                    </telerikGridView:GridViewDataColumn>
                                    <telerikGridView:GridViewColumn Width="32" IsResizable="False" IsFilterable="False" IsReorderable="False">
                                        <telerikGridView:GridViewColumn.CellStyle>

                                            <Style TargetType="grid:GridViewCell">
                                                <Setter Property="Template">
                                                    <Setter.Value>
                                                        <ControlTemplate TargetType="grid:GridViewCell" >
                                                                <Button x:Name="cmdDelSource" Tag="{Binding Source_ID}" Margin="2,2,4,2" HorizontalAlignment="Right"
                                                                        Content="{Binding IsDeleted, Converter={StaticResource helper}}" 
                                                                        ToolTipService.ToolTip="{Binding IsDeleted, Converter={StaticResource SourceTipHelper}}"
                                                                        IsEnabled="{Binding Source={StaticResource isEn}, Path=IsEnabled, Mode=OneWay}" Click="cmdDelSource_Click" ></Button>
                                                        </ControlTemplate>
                                                    </Setter.Value>
                                                </Setter>
                                            </Style>
                                        </telerikGridView:GridViewColumn.CellStyle>
                                        
                                    </telerikGridView:GridViewColumn>

                                    <telerikGridView:GridViewDataColumn Header="Name" Width="3*" DataMemberBinding="{Binding Source_Name}">
                                        <telerikGridView:GridViewDataColumn.AggregateFunctions>
                                            <telerikData:CountFunction Caption="Sources: " />
                                        </telerikGridView:GridViewDataColumn.AggregateFunctions>
                                        <telerikGridView:GridViewDataColumn.CellStyle>
                                            <Style TargetType="grid:GridViewCell">
                                                <Setter Property="Template">
                                                    <Setter.Value>
                                                        <ControlTemplate TargetType="grid:GridViewCell">
                                                            <Border ToolTipService.ToolTip="{Binding Source_Name, Converter={StaticResource SourceNameConverter}}" BorderThickness="{TemplateBinding BorderThickness}"
                                                        BorderBrush="{TemplateBinding BorderBrush}"
                                                        Background="{TemplateBinding Background}" HorizontalAlignment="Stretch">
                                                                <TextBlock Text="{Binding Source_Name, Converter={StaticResource SourceNameConverter}}" 
                                                                           FontStyle="{Binding Source_Name, Converter={StaticResource SourceNameStyleConverter}}"
                                                                           VerticalAlignment="Center" HorizontalAlignment="Left"/>
                                                            </Border>
                                                        </ControlTemplate>
                                                    </Setter.Value>
                                                </Setter>
                                            </Style>
                                        </telerikGridView:GridViewDataColumn.CellStyle>
                                    </telerikGridView:GridViewDataColumn>
                                    <telerikGridView:GridViewDataColumn Header="Items" Width="*" DataMemberBinding="{Binding Total_Items}" IsFilterable="False">
                                        <telerikGridView:GridViewDataColumn.AggregateFunctions>
                                            <telerikData:SumFunction/>
                                        </telerikGridView:GridViewDataColumn.AggregateFunctions>
                                    </telerikGridView:GridViewDataColumn>
                                    <telerikGridView:GridViewDataColumn Header="Deleted Items" Width="*" DataMemberBinding="{Binding Deleted_Items}" IsFilterable="False">
                                        <telerikGridView:GridViewDataColumn.AggregateFunctions>
                                        <telerikData:SumFunction/>
                                        </telerikGridView:GridViewDataColumn.AggregateFunctions>
                                    </telerikGridView:GridViewDataColumn>
                                    <telerikGridView:GridViewDataColumn Header="Duplicates" Width="*" DataMemberBinding="{Binding Duplicates}" IsFilterable="False">
                                        <telerikGridView:GridViewDataColumn.AggregateFunctions>
                                            <telerikData:SumFunction/>
                                        </telerikGridView:GridViewDataColumn.AggregateFunctions>
                                    </telerikGridView:GridViewDataColumn>
                                    <telerikGridView:GridViewDataColumn IsVisible="False" Width="0" DataMemberBinding="{Binding IsDeleted}" IsFilterable="False"></telerikGridView:GridViewDataColumn>
                                    
                                    </telerikGridView:RadGridView.Columns>
                                </telerikGridView:RadGridView>
