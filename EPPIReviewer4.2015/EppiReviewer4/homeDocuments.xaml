                                                        ListItemsWithoutCode="codesTreeControl_ListItemsWithoutCode"
                                                        ListItemsWithoutCodeExcluded="codesTreeControl_ListItemsWithoutCode"
                                                        DisplayItemFrequencies="codesTreeControl_DisplayItemFrequencies"
                                                        AssignSelected="codesTreeControl_AssignSelected"
                                                        RemoveSelected="codesTreeControl_RemoveSelected"
                                                        InsertInDiagram="codesTreeControl_InsertInDiagram"
                                                        InsertChildCodesInDiagram="codesTreeControl_InsertChildCodesInDiagram"
                                                        InsertInReport="codesTreeControl_InsertInReport"
                                                        CodeWithinCode="codesTreeControl_CodeWithinCode"
                                                        AssignSearchSelected="codesTreeControl_AssignSearchSelected"
                                                        RemoveSearchSelected="codesTreeControl_RemoveSearchSelected"
                                                        ></local:CodesTreeControl>
                            </Grid>
                    </radDock:RadDocumentPane>

                        <radDock:RadDocumentPane Header="Sources" Title="Sources/Add" CanDockInDocumentHost="False" PaneHeaderVisibility="Collapsed" CanFloat="False" CanUserPin="False" CanUserClose="False">
