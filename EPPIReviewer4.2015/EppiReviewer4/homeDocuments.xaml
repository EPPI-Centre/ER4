                                </StackPanel>
                                <!--Button x:Name="btRefreshS" Grid.Row="0" Grid.Column="1" FontSize="10" Content="Refresh" 
                                    Width="120" Height="25" Margin="6,6,6,6" Click="btRefreshS_Click"/-->
                            
                                <telerikGridView:RadGridView RowLoaded="grView0_RowLoaded" IsReadOnly="True" ShowGroupPanel="False" RowIndicatorVisibility="Collapsed"
