            <telerikNavigation:RadTabItem x:Name="DuplGr_main_tab" Header="Duplicates Main">
                <Grid ScrollViewer.VerticalScrollBarVisibility="Auto">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="35"></RowDefinition>
                        <RowDefinition Height="auto" MaxHeight="190"></RowDefinition>
                        <RowDefinition Height="2*" ></RowDefinition>
                        <RowDefinition Height="*"></RowDefinition>
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="136" />
                        <ColumnDefinition/>
                    </Grid.ColumnDefinitions>
                    <StackPanel Orientation="Horizontal" Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2" >
                        <Button x:Name="cmdRefreshDuplicateList1" Width="80" Content="Refresh list" Margin="6,6,16,6" Click="cmdRefreshDuplicateList_Click" 
                            ToolTipService.ToolTip="Use this to retrieve the current list of duplicate groups from the server."></Button>
                        <Button x:Name="cmdGetNewDuplicates1" Width="120" Content="Get new duplicates" Margin="6,6,6,6" Click="cmdGetNewDuplicates_Click" 
                                IsEnabled="{Binding Source={StaticResource isEn}, Path=IsEnabled, Mode=OneWay}"
                            ToolTipService.ToolTip="Use this to trigger a full duplicate search. WARNING: this can take a lot of time and should be triggered only after the addition of new items."></Button>
                        <Button x:Name="AutoAssign1" Width="120" Content="Mark Automatically" Margin="6,6,6,6" Click="AutoAssign_Click"  
                                IsEnabled="{Binding Source={StaticResource isEn}, Path=IsEnabled, Mode=OneWay}"
                            ToolTipService.ToolTip="This will automatically mark as duplicate all items that: have a score of 1, that haven't been coded and that don't have any attached document."></Button>
                        <TextBlock x:Name="TextBlockDuplicateListCount1" Margin="6,9,6,6"></TextBlock>
                        <TextBlock x:Name="TextBlockCompletedListCount1" Margin="0,9,6,6"></TextBlock>
                    </StackPanel>

