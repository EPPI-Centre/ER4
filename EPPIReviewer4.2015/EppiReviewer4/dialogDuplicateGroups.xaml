                            <telerikGridView:GridViewDataColumn Header="IsMaster" DataMemberBinding="{Binding IsMaster}" ShowDistinctFilters="False" IsVisible="False"></telerikGridView:GridViewDataColumn>
                        </telerikGridView:RadGridView.Columns>
