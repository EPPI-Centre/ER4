                                <CheckBox Content="Very large magnitude of effect" IsChecked="{Binding UpgradeVeryLarge, Mode=TwoWay}"></CheckBox>
                                <CheckBox Content="All plausible confounding would have reduced the effect" IsChecked="{Binding UpgradeAllPlausible, Mode=TwoWay}"></CheckBox>
                                
                            </StackPanel>
                            <StackPanel Orientation="Vertical" Grid.Column="1">
