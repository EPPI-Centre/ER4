                                <CheckBox Content="Discontinued studies" IsChecked="{Binding PubBiasDiscontinued, Mode=TwoWay}"></CheckBox>
                                <CheckBox Content="Discrepancy published vs. unpublished" IsChecked="{Binding PubBiasDiscrepancy, Mode=TwoWay}"></CheckBox>
                                <CheckBox Content="Other issue" IsChecked="{Binding PubBiasOther, Mode=TwoWay}"></CheckBox>
