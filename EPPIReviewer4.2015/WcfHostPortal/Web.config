    <add key="SMTPAuthentic" value="***REMOVED***" />
    <!--<add key="DomainRoot4Redirect" value="eppi.ioe.ac.uk"/>-->
    <add key="DomainRoot4Redirect" value="eppi.ioe.ac.uk" /><!--in production value should be "eppi.ioe.ac.uk" use only lower case!-->
    <add key="CochraneOAuthBaseUrl" value="https://test-login.cochrane.org/" />
    <!--test env value:!-->
    <add key="CochraneOAuthSS" value="***REMOVED***" />
    <add key="AzureSearchMAGApi-key" value="***REMOVED***"/>
