    function enableSearch() {
        if (document.getElementById("SearchString").value === "") {
            document.getElementById('searchButton').disabled = true;
        } else {
            document.getElementById('searchButton').disabled = false;
        }
    }

    function changeColour(value) {
        switch (value) {

            case '1': 
