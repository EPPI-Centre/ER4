            var colourTheme = sessionStorage.getItem("ColourTheme")

            switch (colourTheme) {

                case '1': // blueish Tints of #68b4f2
                    topColour = "#a4d2f7";
                    botColour = "#d1e8fb";
                    offIcons = "#68b4f2";
                    break;

